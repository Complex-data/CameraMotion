%% Loop through images and save Harris vectors
clc, clear


mQ = 0.0001;
filt = 3;
ROI = [128 1 size(I1,2)-128*2 size(I1,1)];


for k = 1:100
    k
    IL1 = imread(['../ImageData/LeftData/', indexToImageName(index)]);
    IL2 = imread(['../ImageData/LeftData/', indexToImageName(index+1)]);

    IR1 = imread(['../ImageData/LeftData/', indexToImageName(index)]);
    IR2 = imread(['../ImageData/RightData/', indexToImageName(index+1)]);
    [~, ~, ~, ~, ~, ~, mp1, mp2] = returnHarrisVectors(IL1, IL2, mQ, filt, ROI);
    
    Left.matched1{k} = Lmp1;
    Right.matched1{k} = Lmp1;
    
    [~, ~, ~, ~, ~, ~, mp1, mp2] = returnHarrisVectors(IR1, IR2, mQ, filt, ROI);
    
    
    
end